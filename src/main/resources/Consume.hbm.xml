<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.order.model.Consume" table="Consume">


        <id name="id" type="long" column="ID">
            <meta attribute="scope-set">protected</meta>
            <generator class="native" />
        </id>

        <property name="spend" type="int" column="SPEND" not-null="true" />
        <property name="date" type="date" column="DATE" not-null="true" />
        <property name="location" type="string" column="LOCATION" />

        <property name="dishOne" type="string" column="DISHONE" />
        <property name="dishTwo" type="string" column="DISHTWO" />
        <property name="dishThree" type="string" column="DISHTHREE" />
        <property name="dishFour" type="string" column="DISHFOUR" />
        <property name="dishFive" type="string" column="DISHFIVE" />

        </class>
        </hibernate-mapping>