<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.order.model.Expend" table="Expend">


        <id name="id" type="long" column="ID">
            <meta attribute="scope-set">protected</meta>
            <generator class="native" />
        </id>

        <property name="amount" type="int" column="AMOUNT" not-null="true" />
        <property name="date" type="date" column="DATE" not-null="true" />
        <property name="restaurant" type="string" column="RESTAURANT"  />
        <property name="menu" type="string" column="MENU"/>

    </class>
</hibernate-mapping>
