<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.order.model.Dish" table="Dish">

        <id name="id" type="long" column="ID">
            <meta attribute="scope-set">protected</meta>
            <generator class="native" />
        </id>

        <property name="name" type="string" column="NAME" not-null="true" />
        <property name="price" type="int" column="PRICE" not-null="true" />
        <property name="restaurant" type="string" column="RESTAURANT" not-null="true"/>
        <property name="score" type="int" column="SCORE" />
        <property name="times" type="int" column="TIMES" />
        <property name="black" type="boolean" column="BLACK"/>
        <property name="backup" type="string" column="BACKUP"/>
        <property name="type" type="int" column="TYPE" not-null="true"/>

    </class>
</hibernate-mapping>